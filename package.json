        "gh-pages": "6.1.1",
        "husky": "8.0.3",
        "jsdom": "24.0.0",
        "lint-staged": "15.2.0",
        "postcss": "8.4.33",
        "prettier": "3.1.0",
        "tailwindcss": "3.4.1",
        "typescript": "5.2.2",
        "vite": "5.0.8",
        "vitest": "1.0.1"
